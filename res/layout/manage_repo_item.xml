
<RelativeLayout android:id="@+id/relativeLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:paddingRight="10sp" android:layout_width="wrap_content" android:text="TextView"
		android:layout_alignParentLeft="true" android:layout_height="wrap_content"
		android:id="@+id/manage_repo_type"></TextView>
	<TextView android:layout_width="fill_parent" android:text="TextView"
		android:layout_toRightOf="@id/manage_repo_type" android:layout_height="wrap_content"
		android:id="@+id/manage_repo_name" android:layout_alignTop="@id/manage_repo_type"
		android:layout_alignBottom="@id/manage_repo_type"></TextView>
	<TextView android:layout_width="fill_parent" android:text="TextView"
		android:layout_height="wrap_content" android:id="@+id/manage_repo_desc"
		android:layout_below="@id/manage_repo_name" android:layout_alignLeft="@id/manage_repo_name"
		android:layout_alignRight="@id/manage_repo_name"></TextView>
	<TextView android:layout_width="fill_parent" android:text="TextView"
		android:layout_height="wrap_content" android:id="@+id/manage_repo_address"
		android:layout_below="@id/manage_repo_desc" android:layout_alignLeft="@id/manage_repo_desc"
		android:layout_alignRight="@id/manage_repo_desc"></TextView>
</RelativeLayout>
